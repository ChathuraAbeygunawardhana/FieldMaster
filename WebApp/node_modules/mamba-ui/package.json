{
  "name": "mamba-ui",
  "version": "0.0.7",
  "description": "React components",
  "keywords": [
    "react",
    "react-component"
  ],
  "files": [
    "lib",
    "dist"
  ],
  "main": "lib/index.js",
  "scripts": {
    "start": "docz dev",
    "build": "rimraf docs && docz build",
    "compile": "gulp compile",
    "lint": "npm run lint:script && npm run lint:style && npm run lint:tsc",
    "lint:script": "eslint components --ext .ts,.tsx",
    "lint:style": "stylelint components/**/*.less --syntax less",
    "lint:tsc": "npm run tsc",
    "jest": "jest --config .jest.js --no-cache",
    "jest-snap": "jest --config .jest.js --no-cache -- u",
    "prettier": "prettier -c --write **/*",
    "pretty-quick": "pretty-quick",
    "pub": "npm publish --access=public",
    "tsc": "tsc --noEmit",
    "bump": "gulp bump --prerelease"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitee.com/MambaTeam/mamba-ui"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://gitee.com/MambaTeam/mamba-ui/issues"
  },
  "homepage": "https://gitee.com/MambaTeam/mamba-ui",
  "dependencies": {
    "antd": "^3.25.2",
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "viewerjs": "^1.3.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "^7.6.3",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.20",
    "@types/lodash": "^4.14.144",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/viewerjs": "^0.7.2",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "babel-jest": "^24.9.0",
    "docz": "^2.0.0-rc.76",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
	"gatsby-plugin-less": "^3.0.14",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-bump": "^3.1.3",
    "gulp-clean": "^0.4.0",
    "gulp-less": "^4.0.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "jest": "^24.9.0",
    "jsdom": "^15.2.0",
    "jsdom-global": "^3.0.2",
    "less": "^3.10.3",
    "less-plugin-autoprefix": "^2.0.0",
    "merge2": "^1.3.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "rimraf": "^3.0.0",
    "stylelint": "11.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-order": "^3.0.0",
    "through2": "^3.0.1",
